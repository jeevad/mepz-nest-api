<html lang='en'>
<head>
  <meta charset='utf-8' />
  <meta name='viewport' content='width=device-width, initial-scale=1' />
  <title>{{title}}</title>
  <link href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css' rel='stylesheet'
    integrity='sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65' crossorigin='anonymous'
  />
   <style>
    .page-break {
      page-break-before: always;
    }
  </style>
</head>
<body>
  <div class='container-fluid mb-5 mt-0'>
    <div class='row d-flex align-items-baseline'>
      <div class='col-xl-12'>    
        {{#each equipments as |equ_group| }}
          {{#if ../pagewise}} {{#index_gt @index}} <div class="page-break"></div> {{/index_gt}} {{/if}}
           {{#each equ_group as |equ_group_one key2|}} 
          {{!  json equ_group_one }}
          <p style="margin-bottom:2px;"><strong>{{equ_group_one.0.group_labels}} </strong></p>
              {{#each equ_group_one as |data2 key3|}} 
             
              
           {{#if (gt locations.length 0)}}
          
          <p style='color: #242729;font-size: 16px; margin-bottom: 5px;'><b> Equipment:</b> <span>{{code}}</span> <span style="margin-left:30px;">{{name}}</span></p>
          <table class='table table-bordered' style="font-size: 15px; margin-bottom:20px;">
            <thead class='table-light'>
              <tr>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Dept Code</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Department</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Room Code</th>
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Room Name</th>
                  {{#if ../../../fpq_filt}}
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">FPQ</th> {{/if}}
                {{#if ../../../apq_filt}}
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">APQ</th> {{/if}}
                {{#unless ../../../fpq_filt}}
                {{#unless ../../../apq_filt}}
                <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Qty</th>
                 {{/unless}}
                 {{/unless}}
                {{! <th style="padding-top:6px;padding-bottom:6px; color: #242729;">Package</th> }}
                
              </tr>
            </thead>
            <tbody>
              
               
               
            
              {{#each locations as |data|}}
                <tr>
                  <td style="padding-top:6px;padding-bottom:6px">{{data.department.code}}</td>
                  <td style="padding-top:6px;padding-bottom:6px">{{data.department.name}}</td>
                  <td style="padding-top:6px;padding-bottom:6px">{{data.room.code}}</td>
                  <td style="padding-top:6px;padding-bottom:6px">{{data.room.name}}</td>
                    {{#if ../../../../fpq_filt}}
                   <td style="padding-top:6px;padding-bottom:6px;">{{fpq}} </td>
                   {{/if}}
                  {{#if ../../../../apq_filt}}
                   <td style="padding-top:6px;padding-bottom:6px;">{{apq}} </td>
                   {{/if}}
                  {{#unless ../../../../fpq_filt}}
                  {{#unless ../../../../apq_filt}}
                  <td style="padding-top:6px;padding-bottom:6px;">{{qty}} </td>
                   {{/unless}}
                   {{/unless}}
                   {{!-- <td style="padding-top:6px;padding-bottom:6px">{{package1}}</td> --}}
                   
                </tr>      
               {{/each}}
             
             
              
                <tr style="border: 0;">
                  <td style="border:0;"></td>
                  <td style="border:0;"></td>
                  <td style="border:0;"></td>
                   <td style="border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1; padding-top:6px;padding-bottom:6px">Sub-Total:</td>
                    {{#if (and ../../../fpq_filt ../../../apq_filt)}}
                 <td style="padding-top:6px;padding-bottom:6px;border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1;"> {{sum locations "fpq"}}</td>
                  <td style="padding-top:6px;padding-bottom:6px;border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1;"> {{sum locations "apq"}}</td>
                {{else}}
                <td style="padding-top:6px;padding-bottom:6px;border-color: #dfe0e1; border-bottom: 1px solid #dfe0e1;">
                  {{#if ../../../fpq_filt}}
                  {{sum locations "fpq"}} {{/if}} 
                  {{#if ../../../apq_filt}}
                  {{sum locations "apq"}} {{/if}}  
                  {{#unless ../../../fpq_filt}}
                  {{#unless ../../../apq_filt}}  {{sum locations "qty"}} 
                  {{/unless}}
                   {{/unless}}
                  </td>
                  {{/if}}
                 
                 
                </tr>

            </tbody>
          </table>  
         
            {{/if}}
          {{/each}}
          <p style="text-align:right; margin-bottom:0px; color: #242729;"><b>Total Equipment {{equ_group_one.0.group_labels}} : {{ calculateSumOfTotalEquipmentLocationListing equ_group_one }}</b></p>
          {{/each}}
        {{/each}}
        {{!--        <p style="text-align:right; margin-bottom:0px; color: #242729;"><b>Total Equipment : 
          {{!calculateSumOfTotalEquipmentLocationGrpAll equipments }}</b></p> --}}
          <div class="text-center">
          {{#if b_logo}}<img style="height: 30px; width: auto;" src="http://13.232.11.217/assets/images/logo.png">{{/if}}
          {{#if medical_logo}}<img style="height: 30px; width: auto;" src="http://13.232.11.217/assets/images/logo.png">{{/if}}
          {{#if medical_logo2}}<img style="height: 30px; width: auto;" src="http://13.232.11.217/assets/images/logo.png">{{/if}}
          {{#if medical_logo3}}<img style="height: 30px; width: auto;" src="http://13.232.11.217/assets/images/logo.png">{{/if}}
        <div>
      </div>
    </div>
  </div>
</body>
</html>